
.course {
  background-color: #f3f3f3;
  display: flex;
  flex-direction: column;
} 
.course .item{
  width: 100%;
  height: 150px;
  margin-bottom: 10px;
  position: relative;
}
.course .item image{
  width: 100%;
  height: 100%;
  display: block;
}
.course .item .table{
  color: white;
  font-size: 35rpx;
  padding: 10px 30px;
  border: 1px solid white;
  border-radius: 20px;
  position: absolute;
  bottom: 25px;
  cursor: pointer;
}
.course .item .table:nth-of-type(1){
  left: 40px;  
}
.course .item .table:nth-of-type(2){
  right: 40px;
}
